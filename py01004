import math
from multiprocessing.sharedctypes import RawArray
def check(n):
    if n<2: return 0
    for i in range (2,int(math.sqrt(n)) + 1):
        if n%i==0:
            return 0
    return 1


t = int(input())
while(t>0):
    d = 0
    n = int(input())
    for i in range (1,n):
        if(math.gcd(i,n)==1): d += 1
    if(check(d)==1): print("YES")
    else: print("NO") 
    t -= 1
