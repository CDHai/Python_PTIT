t = int(input())
for i in range(t) :
    n = int(input())
    d = 0
    s = "1 "
    x = 2
    while(n != 1) :
        if(n % x == 0) :
            while(n % x == 0) :
                d += 1
                n /= x
        if(d != 0) :
            s += "* " + str(x) + "^" + str(d) + " "
        x += 1
        d = 0
    print(s)

        
    
